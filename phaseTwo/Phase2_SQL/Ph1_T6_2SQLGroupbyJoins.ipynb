    "    table1.column_name, <br>\n",
    "    table2.different_column_name <br>\n",
    "FROM <br>\n",
    "    table1 <br>\n",
    "    INNER JOIN table2 <br>\n",
    "        ON table1.shared_column_name = table2.shared_column_name <br>"
   ]
